    background:rgb(143,146,2);
}

QWidget#toolWidget {
    background:black;
    border-radius: 20px;
}

QLabel#logoLabel1, QLabel#userChoiceLabel, QLabel#indispensableLabel, QLabel#fineGameLabel, QLabel#newAppLabel {
