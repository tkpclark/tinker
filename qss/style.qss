QMainWindow#MainWindow, QWidget#appListWidget {
    background-color: rgb(47,47,47);
}


QWidget#centralwidget {
    background:rgb(101,211,255);
    border-radius: 10px;
}

QLabel#logoLabel {
    color:white;
}

QWidget#userNameWidget, QWidget#passwordWidget, QLineEdit#userNameLineEdit, QLineEdit#passwordLineEdit {
    background:white;
    border-radius: 20px;
}

QLabel#userNameLabel, QLabel#passwordLabel{
    color:rgb(101,211,255);
    font:bold;
}


QWidget#loginWidget,  QPushButton#loginPushButton{
